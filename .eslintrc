{
  "root": true,
  "extends": [
    "airbnb-base"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": ["error"],
    "linebreak-style": ["error", "unix"],
    "no-var": ["error"]
  },
  "env": {
    "es6": true
  },
  "globals": {
    "$": true
  }
}
